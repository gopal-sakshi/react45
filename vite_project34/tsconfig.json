{
    "compilerOptions": {
        "jsx": "react",
        "esModuleInterop": true,
        "allowJs": true,
        "module": "esnext",
        "moduleResolution": "node",
        "allowImportingTsExtensions": true,
        "paths": {
            "@/*": ["src/*"]
        },
        "baseUrl": "./",
    },
    "include": ["lib23/**/*", "src/**/*", "vite.config.ts"],
    "exclude": ["**/node_modules", "**/.*/"],
    "ts-node": {
        "compilerOptions": {
            "module": "commonjs"
        }
    }
}